    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "description": "An Ember codemod to deprecate tryInvoke and replace it with optional chaining",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bachvo/ember-deprecate-tryinvoke-codemod.git"
  },
