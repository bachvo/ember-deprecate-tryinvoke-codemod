{
  "name": "ember-deprecate-tryinvoke-codemod",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "bin": {
    "ember-deprecate-tryinvoke-codemod": "bin/cli.js"
  },
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^3.1.0"
